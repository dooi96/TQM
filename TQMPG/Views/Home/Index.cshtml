    <style>
        html, body{
            font-family: 'Raleway', sans-serif;
        }

        body{
            margin-top: 100px;
        }

        /* section content start*/
        .content{
            margin-top:5%;
            margin-bottom:5%;
        }

        .title{
            font-size: 4rem;
        }

        .content-row{
            margin-left: 10%;
            margin-right: 10%;
        }
        /* section content end*/


        /* Video and News Section Start*/
        .news-section{
            max-height: 680px;
            overflow-y: scroll;
        }

        .video-js{
            width:auto;
        }
        .row-news:nth-child(odd) {
            background: lightblue;
        }

        .row-news:nth-child(even) {
            background: skyblue;
        }
        /* Video and News Section End*/


        /* Best In Class card section Start*/

        .card{
            min-width: 200px;
            border-radius: unset;
            border:none;
            transition: transform .2s, box-shadow .2s;
            margin-bottom:20px;
        }

        .card-body {
            min-width: 200px;
            width:200px;

        }

        .card:hover{
            transform: scale(1.05);
            box-shadow: 0px 0px 6px 2px #888888;
        }

        .bic-col{
            display: grid;
        }

        .card-body img{
            width: inherit;
        }

        /* Best In Class card section End*/

        /* Section parallex Start*/
        .parallex{
            width: 100vw;
            height:500px;
            object-fit: cover;
        }
        /* Section parallex End*/

        /* Success Stories Carousel Start*/
        .image-overlay {
            position: absolute;
            background-color: rgba(255,255,255,0.7);
            width: 100%;
            height: 0px;
            display: block;
            opacity:0;
            transition: height .5s, opacity, padding;
        }

        .img-container:hover .image-overlay{
            height: 100px;
            opacity:1;
            padding:20px;
        }

        .carousel-control-next, .carousel-control-prev{
            width:5%!important;
        }


        /* Success Stories Carousel End*/



        @@media screen and (max-width:980px){
            .card-body{
                margin-left: auto;
                margin-right: auto;
                padding-left: 0;
                padding-right: 0;
            }
        }


    </style>
</head>
